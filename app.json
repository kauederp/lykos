    "android": {
      "package": "com.lykos.lykos",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./modelo/2.png",
        "backgroundColor": "#6b5876"
      }
    },
    "web": {
      "favicon": "./modelo/2.png"
    }
  }
}
